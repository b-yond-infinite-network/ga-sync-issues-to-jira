{
  "name": "sync-jira-subtask-to-gh-issues-action",
  "version": "0.1.0",
  "description": "A github Action to synchronize Github Issues and Jira subtasks",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/index.js",
    "package": "ncc build src/index.js -o dist",
    "test": "eslint src/index.js && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-yond-infinite-network/sync-jira-subtask-to-gh-issues-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "GitHub",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/b-yond-infinite-network/sync-jira-subtask-to-gh-issues-action/issues"
  },
  "homepage": "https://github.com/b-yond-infinite-network/sync-jira-subtask-to-gh-issues-action#readme",
  "dependencies": {
    "@actions/core": "^1.1.1",
    "@actions/github": "^1.0.0",
    "jira-connector": "^2.16.1",
    "lodash": "latest",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.20.5",
    "eslint": "^6.3.0",
    "nock": "^11.7.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run package && git add dist/index.js",
      "pre-push": "npm test"
    }
  }
